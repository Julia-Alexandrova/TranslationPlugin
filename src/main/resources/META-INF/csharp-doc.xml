<idea-plugin>
    <extensions defaultExtensionNs="cn.yiiguxing.plugin.translate">
        <docElementProvider
                language="C#"
                implementationClass="cn.yiiguxing.plugin.translate.provider.CSharpDocumentationElementProvider"/>
    </extensions>

    <extensions defaultExtensionNs="com.intellij">
        <lang.documentationProvider
                language="C#" order="first"
                implementationClass="cn.yiiguxing.plugin.translate.provider.TranslatingDocumentationProvider"/>
    </extensions>
</idea-plugin>
